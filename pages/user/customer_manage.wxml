<view class="record">
    <view class="record_list customer_manage_list">
        <view class="item" wx:for="{{listData}}" bindtap="goDetail" data-id="{{item.id}}" data-oid="{{item.oid}}" data-status="{{item.status}}">
            <view class="f-left">
                <view class="name">
                    {{item.name}}
                </view>
                <view class="time">{{item.phone}}</view>
            </view>
            <view class="price">
                <text class="yuyue">预约时间 </text><text style="font-size:24rpx;">{{item.begin}}-{{item.end}}</text>
            </view>
        </view>
    </view>
    
</view>